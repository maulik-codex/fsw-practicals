class Str_lower 
{ 
    public static void main(String args[]) 
    { 
       
        String str_a = "JAVA PROGRAMING....! "; 
		 System.out.println("String before Conversion:"+str_a); 
 
        String str_b = str_a.toLowerCase(); 

        System.out.println("String after Conversion:"+str_b); 
    } 
}
